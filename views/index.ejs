<html lang="en">
  <%- include('./partials/header')%>

  <body>
    <%- include('./partials/nav')%>

    <div class="blogs content">
      <h2>All Blogs</h2>
      <% if(blogs.length > 0) {%> <% blogs.forEach(blog =>{ %>
      <h3><%= blog.title%></h3>
      <p><%= blog.snippet%></p>
      <% })%> <% } else {%>
      <h3>No Blogs to Display</h3>
      <% }%>
    </div>
    <%- include('./partials/footer')%>
  </body>
</html>
